{
  "name": "@alonetone/stitches",
  "version": "2.0.0",
  "description": "Stitching Together MP3 Playback in HTML5 Audio with ES6",
  "main": "src/playlist.js",
  "scripts": {
    "start": "http-server -s c-1 . & ",
    "test": "npm start && nightwatch -c nightwatch-local.conf.json; npm stop",
    "test:single": "nightwatch -c nightwatch-local.conf.json",
    "stop": "pkill -f http-server",
    "browserstack": "npm start && node browserstack.runner.js --suiteRetries 2 -c nightwatch-browserstack.conf.js -e safari12,safari13,chrome,chromeMac,firefox,firefoxMac,edge,iphoneXS,ios13,ios14,pixel,galaxys9,ucbrowser; npm stop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sudara/stitches.git"
  },
  "files": [
    "src/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "mp3",
    "html5",
    "audio",
    "playback",
    "es6"
  ],
  "author": "Sudara",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sudara/stitches/issues"
  },
  "homepage": "https://github.com/sudara/stitches#readme",
  "private": false,
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "browserstack-local": "1.4.8",
    "chromedriver": "85.0.1",
    "eslint": "7.9.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-import": "2.22.0",
    "http-server": "0.12.3",
    "nightwatch": "1.4.3",
    "prettier": "2.1.2",
    "request": "2.88.2"
  },
  "prettier": {
    "semi": false
  }
}
